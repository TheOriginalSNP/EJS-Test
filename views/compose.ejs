<%- include("partials/header"); -%>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group mb-3">
        <label class="form-label" for="postTitle">Title</label>
        <input class="form-control" type="text" name="postTitle" autocomplete="off">
    </div>
    <div class="form-group mb-3">
        <label class="form-label" for="postContent">Post</label>
        <textarea class="form-control" name="postContent" rows="5" id="postTextArea" autocomplete="off"></textarea>
    </div>
    <button name="newPostButton" type="submit" class="btn btn-primary">Publish</button>
</form>
<%- include("partials/footer"); -%>